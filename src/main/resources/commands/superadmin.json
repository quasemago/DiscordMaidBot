{
  "name": "superadmin",
  "description": "[Owner] Super Admin commands!",
  "default_member_permissions": "8",
  "options": [
    {
      "name": "stop",
      "description": "[Owner] Stop the bot!",
      "type": 1
    },
    {
      "name": "logs",
      "description": "[Owner] Dump the bot logs.",
      "type": 1
    },
    {
      "name": "leaveguild",
      "description": "[Owner] Leave guild",
      "type": 1,
      "options": [
        {
          "name": "guildid",
          "description": "ID of the Guild!",
          "type": 3,
          "required": 1
        }
      ]
    },
    {
      "name": "setlanguage",
      "description": "[Owner] Change the bot language.",
      "type": 1,
      "options": [
        {
          "name": "lang",
          "description": "Language code!",
          "type": 3,
          "required": 1,
          "choices": [
            {
              "name": "English",
              "value": "en-US"
            },
            {
              "name": "Portuguese",
              "value": "pt-BR"
            }
          ]
        }
      ]
    }
  ]
}